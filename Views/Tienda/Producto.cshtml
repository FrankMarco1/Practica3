@model List<Producto>


    <h1>Lista de Productos</h1>

    <table  class="table border">
        <tr>
            <th>Id</th>
            <th>NombreProducto</th>
            <th>Foto</th>
            <th>Decripcion</th>
             <th>Precio</th>
            <th>Celular</th>
            <th>Lugar</th>
            <th>NombreComprador</th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                 <td>@item.Id</td>
            <td>@item.NombreProducto</td>
            <td>
                <a href="@item.Foto">@item.Foto</a>
            </td>

            <td>@item.Decripcion</td>

            <td>@item.Precio</td>

            <td>@item.Celular</td>

            <td>@item.Lugar</td>

            <td>@item.NombreComprador</td>

            </tr>
        }
        @if(Model.Count ==0)
        {
            <tr>
                <td colspan="4" class="text-center">No se encontraron Entrenadores</td>
            </tr>
            
        }
    </table>
    <a href="/Ubicaciones/NuevoEntrenador " class="btn btn-pr